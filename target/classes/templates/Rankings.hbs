{{> templates/base }}
{{#partial "tituloYcss"}}
        <link rel = "stylesheet" href = "css/Rankings.css">
        <title>Rankings</title>
 {{/partial}}
 {{> templates/base }}

 {{#partial "contenido"}}
        <div id="base">
            <h1>Rankings Semanales</h1>
            <div class="tarjetaIncidente">
                <h2>Entidades que solucionan más lento</h2>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col">Entidad</th>
                            <th scope="col">Tipo</th>
                            <th scope="col">Tiempo promedio de resolución</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each filasR1}}
                                    <tr>
                                        {{#each this}}
                                            <td>{{this}}</td>
                                        {{/each}}
                                    </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
            <div class="tarjetaIncidente">
                <h2>Entidades con mayor cantidad de incidentes</h2>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col">Entidad</th>
                            <th scope="col">Tipo</th>
                            <th scope="col">Cantidad de incidentes</th>
                        <tr>
                    </thead>
                    <tbody>
                        {{#each filasR2}}
                                      <tr>
                                          {{#each this}}
                                                <td>{{this}}</td>
                                          {{/each}}
                                      </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
{{/partial}}
{{> templates/base }}
